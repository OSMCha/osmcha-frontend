{
  "name": "osmcha-frontend",
  "version": "1.2.7",
  "type": "module",
  "license": "ISC",
  "engines": {
    "node": ">=7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osmcha/osmcha-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/osmcha/osmcha-frontend/issues"
  },
  "dependencies": {
    "@osmcha/maplibre-adiff-viewer": "^1.3.1",
    "@osmcha/osm-adiff-parser": "^3.0.0",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@turf/area": "^7.1.0",
    "@turf/bbox": "^7.1.0",
    "@turf/bbox-polygon": "^7.2.0",
    "@turf/simplify": "^7.1.0",
    "@turf/truncate": "^7.1.0",
    "date-fns": "^2.22.1",
    "deep-equal": "^2.2.3",
    "diff": "^7.0.0",
    "dompurify": "^3.0.3",
    "lodash.debounce": "^4.0.8",
    "maplibre-gl": "^5.1.0",
    "mousetrap": "^1.6.5",
    "react": "^18.3.1",
    "react-anchorify-text": "^2.4.1",
    "react-click-outside": "^3.0.1",
    "react-datepicker": "^4.1.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.30.3",
    "react-router-dom": "^6.30.3",
    "react-select": "^5.10.1",
    "showdown": "^1.8.6",
    "sonner": "^2.0.7",
    "tag2link": "^2025.5.21",
    "terra-draw": "^1.1.0",
    "terra-draw-maplibre-gl-adapter": "^1.0.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv-cli": "^11.0.0",
    "jsdom": "^27.4.0",
    "mockdate": "^3.0.5",
    "react-test-renderer": "^18.3.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "scripts": {
    "precommit": "npm run check",
    "typecheck": "tsc",
    "build-env": "dotenv -c -- jq -n 'env | with_entries(select(.key | startswith(\"VITE_\")))' > public/env.json",
    "start": "npm run build-env && vite",
    "dev": "npm run start",
    "lint": "biome lint src",
    "format": "biome format src --write",
    "check": "biome check src && npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "build": "npm run build-env && vite build",
    "analyze": "ANALYZE=true npm run build",
    "preview": "vite preview"
  },
  "overrides": {
    "react-anchorify-text": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-notification-system": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "browserslist": [
    ">0.2%",
    "not ie 11",
    "not op_mini all"
  ]
}
